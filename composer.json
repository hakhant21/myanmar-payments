{
    "name": "hakhant/payments",
    "description": "One stop payment integration package for myanmar bank",
    "authors": [
        {
            "name": "Htet Aung Khant",
            "email": "hakhant21@gmail.com",
            "role": "Fullstack Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "firebase/php-jwt": "^6.10",
        "illuminate/support": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hak\\Payments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hak\\Payments\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "license": "MIT"
}
